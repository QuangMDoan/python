    "# roll two six sided dice and then add the numbers together\n",
    "# What's the probability that the sum is equal to 7?\n",
    "import random\n",
    "n = 10000\n",
    "dice = [1, 2, 3, 4, 5, 6]\n",
    "rolls = [1 for i in range(n) if sum([random.choice(dice), random.choice(dice)]) == 7]\n",
    "print(sum(rolls)/n)"
   ]
  }
